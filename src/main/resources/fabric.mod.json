{
	"schemaVersion": 1,
	"id": "acclimatize",
	"version": "${version}",
	"name": "Acclimatize",
	"description": "A highly configurable body temperature system with broad compatibility with modded blocks and items.",
	"authors": [
		"Saint",
		"sparkierkan7"
	],
	"contact": {
		"homepage": "https://github.com/augustsaintfreytag/minecraft-acclimatize",
		"sources": "https://github.com/augustsaintfreytag/minecraft-acclimatize"
	},
	"license": "MIT",
	"icon": "assets/acclimatize/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.saint.acclimatize.Mod"
		],
		"client": [
			"net.saint.acclimatize.ModClient"
		],
		"modmenu": [
			"net.saint.acclimatize.ModMenu"
		]
	},
	"mixins": [
		"acclimatize.mixins.json"
	],
	"depends": {
